{
	"name": "typescript-fsa-redux-thunk",
	"version": "2.1.7",
	"description": "TypeScript FSA utilities for redux-thunk",
	"keywords": [
		"action",
		"action creator",
		"flux standard action",
		"redux",
		"redux-thunk",
		"thunk",
		"typescript",
		"typescript-fsa"
	],
	"homepage": "https://github.com/xdave/typescript-fsa-redux-thunk#readme",
	"bugs": {
		"url": "https://github.com/xdave/typescript-fsa-redux-thunk/issues"
	},
	"repository": "xdave/typescript-fsa-redux-thunk",
	"license": "MIT",
	"author": "Dave Gradwell <deft.productions@gmail.com>",
	"files": [
		"lib",
		"src"
	],
	"main": "lib/index.js",
	"typings": "lib/index.d.ts",
	"scripts": {
		"build": "npm -s run build:lib",
		"build:lib": "tsc",
		"clean": "rimraf lib .nyc_output coverage",
		"lint": "tslint --project tsconfig.json -c tslint.json",
		"test": "cross-env TS_NODE_PROJECT=tsconfig.tests.json nyc mocha --exclude src/examples.ts",
		"posttest": "test $CI && codecov -f coverage/*.json || true",
		"test:watch": "npm -s t -- --reporter min --watch src/**/*.ts --watch-extensions ts"
	},
	"dependencies": {
		"redux-thunk": "^2.3.0",
		"typescript-fsa": "^3.0.0"
	},
	"devDependencies": {
		"@types/chai": "^4.2.3",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.7.5",
		"@types/redux-mock-store": "^1.0.1",
		"chai": "^4.2.0",
		"codecov": "^3.5.0",
		"cross-env": "^6.0.0",
		"isomorphic-fetch": "^2.2.1",
		"mocha": "^6.2.0",
		"nyc": "^14.1.1",
		"redux": "^4.x",
		"redux-mock-store": "^1.5.3",
		"rimraf": "^3.0.0",
		"ts-node": "^8.4.1",
		"tslib": "^1.10.0",
		"tslint": "^5.20.0",
		"typescript": "^3.6.3",
		"typescript-fsa-reducers": "^1.2.1"
	},
	"peerDependencies": {
		"redux": ">=4.x",
		"redux-thunk": "^2.3.x"
	},
	"nyc": {
		"include": [
			"src/**/*.ts"
		],
		"exclude": [
			"src/**/*.spec.ts",
			"src/examples.ts",
			"node_modules"
		],
		"extension": [
			".ts"
		],
		"require": [
			"ts-node/register"
		],
		"reporter": [
			"text-summary",
			"html",
			"json"
		],
		"sourceMap": true,
		"instrument": true
	}
}
